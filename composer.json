{
    "name": "philiagus/figment-container",
    "description": "PHP Figment Framework: Container and DI",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "figment",
        "container"
    ],
    "authors": [
        {
            "name": "Andreas Eicher",
            "email": "philiagus@philiagus.de",
            "role": "lead"
        }
    ],
    "require": {
        "php": ">=8.4",
        "philiagus/parser": "dev-master",
        "psr/container": "~2.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "phpspec/prophecy-phpunit": "*",
        "ext-xdebug": "*"
    },
    "autoload": {
        "psr-4": {
            "Philiagus\\Figment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Philiagus\\Figment\\Test\\": "tests/"
        }
    }
}
